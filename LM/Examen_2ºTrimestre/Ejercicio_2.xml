]>
<lunas>
   <luna numero="N234" nivel="H1"/>
   <luna numero="N479" nivel="H2" planeta="Saturno"/>
   <luna numero="N566" nivel="H2" planeta="Saturno"/>
   <luna numero="N607" nivel="H3" planeta="Urano"/>
